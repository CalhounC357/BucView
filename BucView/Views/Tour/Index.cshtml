@model IEnumerable<Tour>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
<h1>Tours</h1>
<hr />
@{
    foreach (Tour tour in Model) {
        <h1>@tour.Name</h1>
        foreach (TourLocation tl in tour.Locations)
        {
            <h3>@tl.Location?.Name</h3>
            <h4>@tl.Location?.Description</h4>
            <hr />
        }
        <hr />
    }
}
